#include <stdio.h>

int main() {
    int num1, num2, num3, largest;

    printf("Введіть 3 числа: ");
    scanf("%d %d %d", &num1, &num2, &num3);

    if (num1 > num2) {
        largest = num1;
    } else {
        largest = num2;
    }

    if (num3 > largest) {
        largest = num3;
    }

    printf("Найбільше число %d", largest);

    return 0;
}
